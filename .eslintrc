{
  "rules": {
    "no-unused-expressions": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 0,
    "strict": 0,
    "new-cap": 0,
    "camelcase": 0,
    "indent": [
      2,
      4
    ],
    "quotes": [ 2, "single" ],
    "semi": [ 1, "never" ]
  },
  "ecmaFeatures": {
    "jsx": false
  },
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "globals": {
    "BROWSER": false
  },
  "extends": "eslint:recommended"
}